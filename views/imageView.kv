#:kivy 1.0

<ImageView>:
	GridLayout:
		cols: 1		
		GridLayout:
			id: imageGrid
			cols: 1
			size_hint_y: 1.0			
	GridLayout:
		id: overlay
		cols: 2
		padding: 6					
		size_hint_y: 1.0					
		opacity: 0
		ImageButton:			
			id: backButton
			size_hint_x: None	
			width: '30sp'
			size_hint_y: None
			height: '30sp'
			source: 'images\\arrow-left.png'		